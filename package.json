{
  "name": "gatsby-source-prismic-field",
  "version": "0.0.3",
  "description":
    "Gatsby source plugin for building websites using prismic.io as a data source. Forked from gatsby-source-prismic.",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir .",
    "format":
      "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "babel -w src --out-dir ."
  },
  "homepage": "https://github.com/fieldmedialab/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/fieldmedialab/gatsby-source-prismic.git"
  },
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin"],
  "author":
    "Angelo Ashmore <angelo.ashmore@walltowall.com>, Field Media Lab <hello@fieldmedialab.com>",
  "license": "MIT",
  "dependencies": {
    "gatsby-node-helpers": "^0.1.3",
    "lodash": "^4.17.4",
    "prismic-javascript": "^1.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "prettier": "^1.5.3"
  }
}
